<Window x:Class="WpfGebruiker.WindowToevoegen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfGebruiker"
        mc:Ignorable="d"
        Title="WPF Buurlenen" Height="800" Width="670">
    <Grid>

        <Grid.RowDefinitions>
            <RowDefinition Height="80"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <StackPanel>
            <Button x:Name="btnGem" Grid.Row="0" Height="55" Width="140" Margin="-300,20,0,0" Content="Gemotoriseerd" Click="btnGem_Click"/>
            <Button x:Name="btnGet" Grid.Row="0" Height="55" Width="140" Margin="50,-55,0,0" Content="Getrokken" Click="btnGet_Click"/>
        </StackPanel>
        <Frame x:Name="Mainn" Grid.Row="1" NavigationUIVisibility="Hidden"></Frame>

    </Grid>
</Window>
