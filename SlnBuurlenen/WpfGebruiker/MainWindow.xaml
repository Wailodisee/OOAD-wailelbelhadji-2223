<Window x:Class="WpfGebruiker.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfGebruiker"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Vertical" VerticalAlignment="Top" Margin="15,60">
            <Button Content="Home" HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="0,10" MinWidth="110" FontSize="16"></Button>
            <Button Content="Ontleningen" HorizontalAlignment="Left" MinWidth="110" FontSize="16"></Button>
            <Button Content="Voertuigen" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="0,10" MinWidth="110" FontSize="16"></Button>
        </StackPanel>
        <Frame x:Name="Main" Margin="130,70,0,0"></Frame>
    </Grid>
    
    
</Window>
